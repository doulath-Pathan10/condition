const AsiaMiles: React.FC = () => {
  const [selectedCard, setSelectedCard] = useState(null);

  function buttonclick() {
    startapp();
  }
  
  function startapp() {
    var url = window.location.href;
    var hash = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
    var language = "en";
    var scb_token = "";
    var source = "";
    var offerCode = "";
    
    for (var i = 0; i < hash.length; i++) {
      if (hash[i].indexOf("scbAccessToken") != -1) {
        scb_token = hash[i].split('=')[1];
      }
      else if (hash[i].indexOf("source") != -1) {
        source = hash[i].split('=')[1];
      }
      if (hash[i].indexOf("lang") != -1) {
        language = hash[i].split('=')[1];
      }
      if (hash[i].indexOf("offer") != -1) {
        offerCode = hash[i].split('=')[1];
      }
    }
    if (scb_token != null) {
      sessionStorage.setItem("landingURL", window.location.href);
    }
    var rand = Math.floor((1 + Math.random()) * 0x10000);
    //T0 Product
    if (selectedCard === "t0") {
      if (scb_token.length > 0) { //With SCB token
        if (offerCode == "A00") {
          window.open("index.html?product=1313&campaign=HKSOZ00MCWB000&premium=CT00&cardType=3268&affiliation=2132&channelName=ibk_pre_am +&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A01") {
          window.open("index.html?product=1313&campaign=HKSOZ00MCWB000&premium=CO00&cardType=3268&affiliation=2132&channelName=ibk_pre_am +&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A02") {
          window.open("index.html?product=1313&campaign=HKSOZ00MCWB000&premium=CT30&cardType=3268&affiliation=2132&channelName=ibk_pre_am +&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A03") {
          window.open("index.html?product=1313&campaign=HKSOZ00MCWB000&premium=CT00&cardType=3268&affiliation=2132&channelName=ibk_pre_am +&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A04") {
          window.open("index.html?product=1313&campaign=HKSOZ00MCWB000&premium=CT40&cardType=3268&affiliation=2132&channelName=ibk_pre_am +&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A05") {
          window.open("index.html?product=1313&campaign=HKSOZ00MCWB000&premium=CT70&cardType=3268&affiliation=2132&channelName=ibk_pre_am +&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else {
          window.open("index.html?product=1313&campaign=HKSOZ00MCWB000&premium=CT00&cardType=3268&affiliation=2132&channelName=ibk_pre_am +&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
      }
      else  if (offerCode == "A00") {
        window.open("index.html?product=1313&campaign=HKSOZ00MCWB000&premium=CT20&cardType=3268&affiliation=2132&channelName=ibk_pre_am +&lang=" + language + "&source=" + source, "_self");
      }
      else if (offerCode == "A01") { //Without SCB token
        window.open("index.html?product=1313&campaign=HKSOZ00MCWB000&premium=CO00&cardType=3268&affiliation=2132&channelName=ibk_pre_am +lang" + language + "&source=" + source, "_self");
      }
      else if (offerCode == "A02") {
        window.open("index.html?product=1313&campaign=HKSOZ00MCWB000&premium=CT30&cardType=3268&affiliation=2132&channelName=ibk_pre_am +lang" + language + "&source=" + source, "_self");
      }
      else if (offerCode == "A03") {
        window.open("index.html?product=1313&campaign=HKSOZ00MCWB000&premium=CT00&cardType=3268&affiliation=2132&channelName=ibk_pre_am +lang" + language + "&source=" + source, "_self");
      }
      else if (offerCode == "A04") {
        window.open("index.html?product=1313&campaign=HKSOZ00MCWB000&premium=CT40&cardType=3268&affiliation=2132&channelName=ibk_pre_am +lang" + language + "&source=" + source, "_self");
      }
      else if (offerCode == "A05") {
        window.open("index.html?product=1313&campaign=HKSOZ00MCWB000&premium=CT70&cardType=3268&affiliation=2132&channelName=ibk_pre_am +lang" + language + "&source=" + source, "_self");
      }
      else {
