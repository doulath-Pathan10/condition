    //T2 Product
    if (selectedCard === 't2') {
      if (scb_token.length > 0) { //With SCB token
        if (offerCode == "A00") {
          window.open("index.html?product=1313&campaign=HKSOZ02MCWB000&premium=CT02&cardType=3270&affiliation=2132&channelName=ibk_pre_am +&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A01") {
          window.open("index.html?product=1313&campaign=HKSOZ02MCWB000&premium=CO02&cardType=3270&affiliation=2132&channelName=ibk_pre_am +&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A02") {
          window.open("index.html?product=1313&campaign=HKSOZ02MCWB000&premium=CT32&cardType=3270&affiliation=2132&channelName=ibk_pre_am +&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A03") {
          window.open("index.html?product=1313&campaign=HKSOZ02MCWB000&premium=CS12&cardType=3270&affiliation=2132&channelName=ibk_pre_am +&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A04") {
          window.open("index.html?product=1313&campaign=HKSOZ02MCWB000&premium=CT42&cardType=3270&affiliation=2132&channelName=ibk_pre_am +&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A05") {
          window.open("index.html?product=1313&campaign=HKSOZ02MCWB000&premium=CT72&cardType=3270&affiliation=2132&channelName=ibk_pre_am +&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else {
          window.open("index.html?product=1313&campaign=HKSOZ02MCWB000&premium=CT72&cardType=3270&affiliation=2132&channelName=ibk_pre_am +&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
          // window.open("/onboarding/hk-uat/www/rtoForms/index.html?rand=" + rand + "#/onboarding?country=HK&sourceName=asiamiles&products={'product_category':'CC','product_type':'1313','campaign':'HKSOZ02MCWB000','name':'Standard Chartered Cathay Mastercard – Priority Private'}&channelName=ibk_pre_am&cardType=3270&affiliation=2132&pageId=pd-1&premium=CT02&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
      }
      else  if (offerCode == "A00") {
        window.open("index.html?product=1313&campaign=HKSOZ02MCWB000&premium=CT02&cardType=3270&affiliation=2132&channelName=ibk_pre_am +lang" + language + "&source=" + source, "_self");
        }
      else if (offerCode == "A01") { //Without SCB token
        window.open("index.html?product=1313&campaign=HKSOZ02MCWB000&premium=CO02&cardType=3270&affiliation=2132&channelName=ibk_pre_am +lang" + language + "&source=" + source, "_self");
      }
      else if (offerCode == "A02") {
        window.open("index.html?product=1313&campaign=HKSOZ02MCWB000&premium=CT32&cardType=3270&affiliation=2132&channelName=ibk_pre_am +lang" + language + "&source=" + source, "_self");
      }
      else if (offerCode == "A03") {
        window.open("index.html?product=1313&campaign=HKSOZ02MCWB000&premium=CS12&cardType=3270&affiliation=2132&channelName=ibk_pre_am +lang" + language + "&source=" + source, "_self");
      }
      else if (offerCode == "A04") {
        window.open("index.html?product=1313&campaign=HKSOZ02MCWB000&premium=CT42&cardType=3270&affiliation=2132&channelName=ibk_pre_am +lang" + language + "&source=" + source, "_self");
      }
      else if (offerCode == "A05") {
        window.open("index.html?product=1313&campaign=HKSOZ02MCWB000&premium=CT72&cardType=3270&affiliation=2132&channelName=ibk_pre_am +lang" + language + "&source=" + source, "_self");
      }
      else {
        window.open("index.html?product=1313&campaign=HKSOZ02MCWB000&premium=CT00&cardType=3270&affiliation=2132&channelName=ibk_pre_am +lang" + language + "&source=" + source, "_self");
      }
    }
  }

  // useEffect(() => {
  //   asiaMilesJson();
  // }, []);

  const handleSelectCard = (id: any) => {
    setSelectedCard(id);
  };

  return (
    <>
      <div className="header-container">
        <header className="header">
          <div className="column01">
            <div className="col-sm">
              <p id="applyingfor">You are applying for</p>
              <span id="header">Standard Chartered Cathay Mastercard</span>
            </div>
            <div className="column2">
              <hr />
              <p className="credit">
                To apply for Standard Chartered credit card, you need to be 18+
                years old, and a Hong Kong resident
              </p>
            </div>
          </div>
        </header>
      </div>
      <div className="credit-card-selector">
        <div className="col-smtr">
          <p className="select-maincard">Please select one type of credit card</p>
        </div>
        <div className="cards-container">
          {cardData.map((card:any) => (
            <div
              key={card.id}
              className={`card ${selectedCard === card.id ? "selected" : ""}`}
              // onClick={() => handleSelectCard(card?.id)}
            >
              <div className="main-image">
                <img src={card.image} />
              </div>
              <div className="card-content-main">
                <h3 className="card-content-title">{card.title}</h3>
                <div className="card-content">
                  <ul>
                    {card.description.map((item:any, index:any) => (
                      <li key={index} className="card-content-text">
                        {item}
                      </li>
                    ))}
                  </ul>
                  <div className="button-container">
                    {selectedCard === card.id ? (
                      <div className="selected-label">
                        <span className="tick">
                          <svg
                            xmlns="http://www.w3.org/2000/svg"
                            viewBox="0 0 24 24"
                            fill="none"
                            stroke="#007bff"
                            stroke-width="2"
                            stroke-linecap="round"
                            stroke-linejoin="round"
                          >
                            <polyline points="20 6 9 17 4 12" />
                          </svg>
                        </span>{" "}
                        Selected
                      </div>
                    ) : (
                      <div className="select-button" onClick={() => handleSelectCard(card?.id)}>Select</div>
                    )}
                  </div>
                </div>
              </div>
            </div>
          ))}
        </div>

        <p className="footermsg">
          Your Integrated Deposit Account and Banking Plan application would be
          handled in the process as well&nbsp;(if applicable)
        </p>
        <button
          className="continue-button"
          disabled={selectedCard === null}
          onClick={() => buttonclick()}
        >
          OK, NEXT
        </button>
      </div>
    </>
  );
}

export default AsiaMiles;


