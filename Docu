import { KeyWithAnyModel, ValidationObjModel } from '../../utils/model/common-model';
import rulesUtils from './rules.utils';

const Rules_bd_4 = (props: KeyWithAnyModel, application: KeyWithAnyModel, employeeToggleSelector?: any): KeyWithAnyModel => {
    const validationObj: ValidationObjModel = {
        nonEditable: [],
        hidden: []
    };

    if(employeeToggleSelector && employeeToggleSelector.deleteFields.length > 0) {
        const hidden: Array<string> = [employeeToggleSelector.deleteFields[0]]
        validationObj.hidden.push(hidden)
    }

    return rulesUtils(props, validationObj);
}

export default Rules_bd_4;

