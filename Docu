import { createSlice } from "@reduxjs/toolkit";

const lastAccessed = createSlice({
    name:'lastAccessed',
    initialState: {
        fieldFocused: null
    },
    reducers: {
        getField(state, action) {
            state.fieldFocused = action.payload
        }
    }
});

export const lastAction = lastAccessed.actions;

export default lastAccessed;
